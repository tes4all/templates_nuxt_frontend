{
  "name": "templates_nuxt_frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "upgrade": "pnpm upgrade --latest --save-exact",
    "dev": "nuxt dev",
    "build": "NUXT_SITE_ENV=preview nuxt build --mode testsystem",
    "build:prod": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "sls": "serverless --stage dev",
    "sls:prod": "serverless --stage prod",
    "deploy": "pnpm build && pnpm sls s3deploy && pnpm sls deploy",
    "deploy:prod": "pnpm build && pnpm sls:prod s3deploy && pnpm sls:prod deploy"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@nuxt/content": "^3.5.1",
    "@nuxt/kit": "^3.17.5",
    "@nuxt/schema": "^3.17.5",
    "@tailwindcss/vite": "^4.1.8",
    "@vueuse/core": "^13.3.0",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "express": "^5.1.0",
    "lucide-vue-next": "^0.514.0",
    "pinia": "^3.0.3",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@nuxtjs/seo": "3.0.3",
    "@tes/serverless-default": "npm:@jsr/tes__serverless-default@^0.3.2",
    "@types/node": "^24.0.0",
    "@types/serve-static": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vue/server-renderer": "^3.5.16",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "jsdoc-type-pratt-parser": "^4.1.0",
    "nuxt": "^3.17.5",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-security": "^2.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "serverless": "^4.17.0",
    "serverless-cloudfront-invalidate": "^1.12.2",
    "serverless-http": "^3.2.0",
    "serverless-plugin-lambda-insights": "^2.0.0",
    "serverless-s3-deploy": "^0.10.1",
    "shadcn-nuxt": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "unplugin-vue-markdown": "^28.3.1",
    "vite": "^6.3.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^10.1.3"
  },
  "pnpm": {
    "overrides": {
      "@aws-sdk/types": "^3.535.0"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
